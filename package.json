{
    "name": "@owlprotocol/solidity-starter",
    "version": "0.1.3",
    "private": false,
    "repository": {
        "type": "git",
        "url": "https://github.com/owlprotocol/solidity-starter.git"
    },
    "homepage": "https://owlprotocol.github.io/solidity-starter",
    "copyright": "Copyright 2021 Owl Protocol",
    "license": "MIT",
    "scripts": {
        "hardhat:compile": "npx hardhat compile",
        "hardhat:deploy": "npx hardhat run --network localhost scripts/deploy.ts",
        "hardhat:node": "npx hardhat node",
        "clean": "npm-run-all -p clean:scripts clean:test clean:types",
        "clean:scripts": "rimraf scripts",
        "clean:test": "rimraf test",
        "clean:types": "rimraf typechain",
        "test": "npx hardhat test",
        "start": "npx hardhat run --network localhost scripts/deploy.t",
        "test:parallel": "npx hardhat test --parallel",
        "lint": "eslint scripts test",
        "lint:fix": "eslint scripts test --fix",
        "lint:staged": "lint-staged",
        "docs:solidity": "solidity-docgen --solc-module solc-0.8 -i contracts -o docs/solidity",
        "docs:ts": "typedoc",
        "prepare": "husky install && npm run hardhat:compile",
        "prepublishOnly": "npm run test"
    },
    "dependencies": {
        "dotenv": "^10.0.0"
    },
